<!-- Portfolio Grid-->
<section class="page-section bg-light" id="portfolio">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Portfolio</h2>
            <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
        </div>
        <div class="row">
            @foreach(var items in Model)
            {
                <div class="col-lg-4 col-sm-6 mb-4">
                    <!-- Portfolio item 1-->
                    <div class="portfolio-item" style="width: 350px; height: 300px;">
                        <a class="btn btn-primary portfolio-link" data-bs-toggle="modal">
                            <div class="portfolio-hover">
                                <div class="portfolio-hover-content"><i class="fas fa-plus fa-3x"></i></div>
                            </div>
                            <img class="img-fluid" src="~/Image/@items.Image3" alt="..." style="width: 100%; height: 100%; object-fit: cover;" />
                        </a>
                        <div class="portfolio-caption">
                            <a class="btn btn-primary" asp-action="Details" asp-controller="Farmer" asp-route-id="@items.Id" type="submit">Submit</a>
                            @* <button asp-action="HomeDetails" asp-controller="Farmer" type="submit">Threads</button> *@
                            <div class="portfolio-caption-subheading text-muted">Illustration</div>
                        </div>
                    </div>
                </div>



            }
         
        </div>
    </div>
</section>
